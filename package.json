{
  "name": "belt-wizard",
  "description": "Belt customization wizard app for BeltMaster",
  "license": "MIT",
  "private": true,
  "author": "Chance Snow <chance@snow.llc>",
  "contributors": [
    "Hayden Anderson <hayden@jollywebconsulting.com>"
  ],
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "tsc && wtr --coverage",
    "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
    "storybook": "tsc && npm run analyze -- --exclude dist && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"storybook dev -p 8080\"",
    "storybook:build": "tsc && npm run analyze -- --exclude dist && storybook build",
    "analyze": "cem analyze --litelement"
  },
  "dependencies": {
    "@adobe/lit-mobx": "^2.2.2",
    "@shopify/storefront-api-client": "^1.0.9",
    "@std/async": "npm:@jsr/std__async@^1.0.14",
    "lit": "^3.1.4",
    "mobx": "^6.13.7",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@open-wc/testing": "^4.0.0",
    "@std/assert": "npm:@jsr/std__assert",
    "@types/mocha": "^10.0.10",
    "@web/test-runner": "^0.18.2",
    "concurrently": "^8.2.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^7.1.7"
  },
  "customElements": "custom-elements.json"
}
